{
    // 使用 IntelliSense 了解相关属性。 
    // 悬停以查看现有属性的描述。
    // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        
       
        {
            "type": "extensionHost",
            "request": "launch",
            "name": "Launch Extension",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "preLaunchTask": "npm"
        },
        {
            "name": "Python: Terminal (integrated)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "PowerShell Launch ",
            "script": "${workspaceFolder}/",
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/app.js"
        },
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },

        {
            "type": "java",
            "name": "Debug (Launch)",
            "request": "launch",
            "mainClass": ""
        },
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "name": "Flutter",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Docker: Attach to Node",
            "remoteRoot": "/usr/src/app"
        },
        {
            "name": "Dart",
            "type": "dart",
            "request": "launch",
            "program": "bin/main.dart"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
                    // 配置名称，在启动配置下拉菜单中显示
                    "name": "(gdb) Launch", 
                    // 调试会话开始前要运行的任务
                    "preLaunchTask": "build", 
                    // 配置类型
                    "type": "cppdbg", 
                    // 请求配置类型，可以为启动（launch）或附加（attach）
                    "request": "launch", 
                    // 将要进行调试的程序的路径
                    "program": "${fileDirname}/${fileBasenameNoExtension}.exe", 
                    // 程序调试时传递给程序的命令行参数，一般设为空即可
                    "args": [], 
                    // 设为true时程序将暂停在程序入口处，一般设置为false
                    "stopAtEntry": false, 
                    // 调试程序时的工作目录，一般为${workspaceFolder}即代码所在目录
                    "cwd": "${workspaceFolder}", 
                    "environment": [],
                    // 调试时是否显示控制台窗口，一般设置为true显示控制台
                    "externalConsole": true, 
                    "MIMode": "gdb",
                    // GDB的路径，注意替换成自己的位置中的 gdb
                    "miDebuggerPath": "C:/Program Files/MiniGW/mingw64/bin/gdb.exe", 
                    "setupCommands": [
                        {
                            "description": "Enable pretty-printing for gdb",
                            "text": "-enable-pretty-printing",
                            "ignoreFailures": true
                        }
                    ]
                }
            ]
        }